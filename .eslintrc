{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": 2,
    "prefer-const": 0,
    "indent": ["error", "tab"],
    "no-tabs": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "no-else-return": 0,
    "eqeqeq": 1,
    "no-bitwise": 1
  },
  "env": {
    "browser" : true,
    "node": true
  }
}
